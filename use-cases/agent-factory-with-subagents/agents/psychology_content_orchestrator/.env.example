# Psychology Content Orchestrator Agent Environment Configuration

# LLM Provider Settings
LLM_PROVIDER=openai
LLM_API_KEY=your_llm_api_key_here
LLM_MODEL=qwen2.5-coder-32b-instruct
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Project Configuration
PROJECT_NAME=New Psychology Project
PROJECT_DESCRIPTION=Psychology testing project
TEST_TOPICS=["depression", "anxiety", "stress"]
TARGET_AUDIENCE=general
COMPLEXITY_LEVEL=standard
TARGET_LANGUAGE=ukrainian

# Orchestration Settings
ORCHESTRATION_LEVEL=full_pipeline
WORKFLOW_COMPLEXITY=standard
ENABLE_PARALLEL_EXECUTION=true
MAX_PARALLEL_AGENTS=3
COORDINATION_TIMEOUT=300

# Quality Gates
PATTERNSHIFT_THRESHOLD=0.85
CLINICAL_ACCURACY_THRESHOLD=0.90
VAK_ADAPTATIONS_THRESHOLD=0.80
LANGUAGE_QUALITY_THRESHOLD=0.85

# Monitoring and Reporting
ENABLE_PROGRESS_TRACKING=true
ENABLE_DETAILED_LOGGING=true
REPORT_FREQUENCY=per_stage

# Archon Integration
ARCHON_PROJECT_ID=c75ef8e3-6f4d-4da2-9e81-8d38d04a341a
ENABLE_RAG_SEARCH=true
RAG_MATCH_COUNT=5

# Optional: Alternative LLM Providers
# GEMINI_API_KEY=your_gemini_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Task Queue Configuration (for distributed orchestration)
# REDIS_URL=redis://localhost:6379
# CELERY_BROKER_URL=redis://localhost:6379
# CELERY_RESULT_BACKEND=redis://localhost:6379