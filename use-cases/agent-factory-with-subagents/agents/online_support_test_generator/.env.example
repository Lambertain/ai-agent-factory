# Psychology Test Generator Agent - Environment Configuration Example

# ===== LLM CONFIGURATION =====
LLM_PROVIDER=openai
LLM_API_KEY=your_api_key_here
LLM_MODEL=qwen2.5-coder-32b-instruct
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Specialized models for different tasks
LLM_ANALYSIS_MODEL=qwen2.5-72b-instruct
LLM_VALIDATION_MODEL=qwen2.5-coder-7b-instruct

# ===== TEST GENERATION DEFAULTS =====
DEFAULT_PSYCHOLOGICAL_DOMAIN=general
DEFAULT_TARGET_POPULATION=adults
DEFAULT_TEST_TYPE=assessment
DEFAULT_MEASUREMENT_PURPOSE=screening

# ===== QUALITY CONTROL =====
MIN_QUESTION_COUNT=5
MAX_QUESTION_COUNT=100
MIN_RELIABILITY_THRESHOLD=0.70
MAX_RELIABILITY_THRESHOLD=0.95

# ===== ARCHON INTEGRATION =====
ARCHON_PROJECT_ID=c75ef8e3-6f4d-4da2-9e81-8d38d04a341a
ENABLE_RAG_SEARCH=true
RAG_MATCH_COUNT=5

# ===== FILE SYSTEM =====
OUTPUT_DIRECTORY=./generated_tests
KNOWLEDGE_DIRECTORY=./knowledge
TEMPLATES_DIRECTORY=./templates

# ===== VALIDATION SETTINGS =====
ENABLE_AUTOMATIC_VALIDATION=true
ENABLE_PSYCHOMETRIC_ANALYSIS=true
ENABLE_BIAS_DETECTION=true
ENABLE_READABILITY_CHECK=true

# ===== AGENT INTEGRATION =====
ENABLE_RESEARCH_INTEGRATION=true
ENABLE_ARCHITECT_COLLABORATION=true
ENABLE_QUALITY_ASSURANCE=true

# ===== LOCALIZATION =====
DEFAULT_LANGUAGE=ru
SUPPORTED_LANGUAGES=ru,en,es,fr,de

# ===== SECURITY & ETHICS =====
ENABLE_ETHICS_CHECK=true
ENABLE_CONFIDENTIALITY_PROTECTION=true
ENABLE_INFORMED_CONSENT=true

# ===== PERFORMANCE =====
MAX_CONCURRENT_GENERATIONS=3
GENERATION_TIMEOUT_SECONDS=300
CACHE_ENABLED=true

# ===== ALTERNATIVE PROVIDERS (Optional) =====
# GEMINI_API_KEY=your_gemini_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here

# ===== ADVANCED FEATURES (Optional) =====
# ENABLE_ADAPTIVE_TESTING=false
# ENABLE_REAL_TIME_SCORING=false
# ENABLE_MULTILINGUAL_GENERATION=false