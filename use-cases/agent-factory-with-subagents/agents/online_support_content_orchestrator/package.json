{
  "name": "psychology-content-orchestrator",
  "version": "1.0.0",
  "description": "Universal Psychology Content Orchestrator Agent for coordinating multi-agent psychological content creation",
  "main": "core/orchestrator.js",
  "scripts": {
    "start": "node examples/usage-examples.js",
    "test": "node test/test-runner.js",
    "example:therapeutic": "node -e \"require('./examples/usage-examples').createTherapeuticProgram()\"",
    "example:assessment": "node -e \"require('./examples/usage-examples').createEducationalAssessment()\"",
    "example:organizational": "node -e \"require('./examples/usage-examples').createOrganizationalWellnessProgram()\"",
    "example:research": "node -e \"require('./examples/usage-examples').createResearchFramework()\"",
    "example:custom": "node -e \"require('./examples/usage-examples').demonstrateCustomDomain()\"",
    "example:multi-domain": "node -e \"require('./examples/usage-examples').demonstrateMultiDomainIntegration()\"",
    "example:monitoring": "node -e \"require('./examples/usage-examples').demonstrateStatusMonitoring()\"",
    "docs": "node docs/generate-docs.js",
    "validate": "node utils/validate-config.js"
  },
  "keywords": [
    "psychology",
    "content-orchestrator",
    "multi-agent",
    "ai-coordination",
    "therapeutic-content",
    "assessment-tools",
    "psychological-interventions",
    "evidence-based",
    "universal-agent",
    "workflow-management"
  ],
  "author": "Psychology Content Orchestrator Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psychology-ai/content-orchestrator.git"
  },
  "bugs": {
    "url": "https://github.com/psychology-ai/content-orchestrator/issues"
  },
  "homepage": "https://github.com/psychology-ai/content-orchestrator#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "events": "^3.3.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.8.0",
    "nodemon": "^2.0.20"
  },
  "peerDependencies": {
    "mcp-archon-client": "^1.0.0",
    "rag-connector": "^1.0.0"
  },
  "config": {
    "defaultDomain": "general-psychology",
    "defaultStrategy": "standard",
    "defaultThreshold": 0.8,
    "maxConcurrentWorkflows": 5,
    "defaultTimeout": 300000
  },
  "directories": {
    "core": "./core",
    "config": "./config",
    "integrations": "./integrations",
    "utils": "./utils",
    "examples": "./examples",
    "docs": "./docs",
    "test": "./test"
  },
  "exports": {
    ".": "./core/orchestrator.js",
    "./core/*": "./core/*.js",
    "./config/*": "./config/*.js",
    "./integrations/*": "./integrations/*.js",
    "./utils/*": "./utils/*.js",
    "./examples/*": "./examples/*.js"
  },
  "files": [
    "core/",
    "config/",
    "integrations/",
    "utils/",
    "examples/",
    "docs/",
    "README.md",
    "LICENSE"
  ]
}