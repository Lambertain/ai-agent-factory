# Queue/Worker Management Agent Environment Configuration

# =============================================================================
# LLM Configuration (Required)
# =============================================================================

# Primary LLM Provider Settings
LLM_API_KEY=your_llm_api_key_here
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
LLM_MODEL=qwen2.5-coder-32b-instruct

# Cost-optimized model configuration for different queue/worker tasks
LLM_ARCHITECTURE_MODEL=qwen2.5-72b-instruct
LLM_IMPLEMENTATION_MODEL=qwen2.5-coder-32b-instruct
LLM_DOCS_MODEL=gemini-2.5-flash-lite
LLM_MONITORING_MODEL=qwen2.5-coder-7b-instruct

# Alternative provider for cost-effective tasks
GEMINI_API_KEY=your_gemini_api_key_here

# =============================================================================
# Agent Configuration
# =============================================================================

# Agent Identity
AGENT_NAME=Queue/Worker Management Agent
AGENT_VERSION=1.0.0

# Default Queue/Worker Settings
DEFAULT_QUEUE_TYPE=redis
DEFAULT_WORKER_TYPE=celery
DEFAULT_USE_CASE=api_processing
DEFAULT_DEPLOYMENT=standalone

# =============================================================================
# RAG and Knowledge Base
# =============================================================================

# Archon Knowledge Base Integration
ARCHON_URL=http://localhost:3737
KNOWLEDGE_BASE_ENABLED=true

# Default knowledge tags for queue/worker management
DEFAULT_KNOWLEDGE_TAGS=queue-worker,agent-knowledge,pydantic-ai

# =============================================================================
# Performance Settings
# =============================================================================

# LLM Performance
MAX_RETRIES=3
TIMEOUT_SECONDS=60
ENABLE_CACHING=true

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60
MAX_REQUESTS_PER_HOUR=1000

# =============================================================================
# Development Settings
# =============================================================================

# Debug and Logging
DEBUG_MODE=false
LOG_LEVEL=INFO
ENABLE_VERBOSE_LOGGING=false

# Development Features
HOT_RELOAD=true
AUTO_SAVE=true

# =============================================================================
# Security Settings
# =============================================================================

# Input Validation
ENABLE_INPUT_VALIDATION=true
MAX_INPUT_LENGTH=10000
SANITIZE_INPUTS=true

# Security Defaults
DEFAULT_QUEUE_AUTHENTICATION=false
DEFAULT_SSL_ENABLED=false

# =============================================================================
# Queue System Configuration
# =============================================================================

# Redis Configuration
REDIS_DEFAULT_URL=redis://localhost:6379/0
REDIS_CONNECTION_POOL_SIZE=10
REDIS_SOCKET_TIMEOUT=30

# RabbitMQ Configuration
RABBITMQ_DEFAULT_URL=amqp://guest:guest@localhost:5672//
RABBITMQ_HEARTBEAT=600
RABBITMQ_BLOCKED_CONNECTION_TIMEOUT=300

# AWS SQS Configuration
AWS_SQS_REGION=us-east-1
AWS_SQS_VISIBILITY_TIMEOUT=300
AWS_SQS_MESSAGE_RETENTION=345600

# Azure Service Bus Configuration
AZURE_SERVICEBUS_NAMESPACE=
AZURE_SERVICEBUS_AUTH_TIMEOUT=60

# Google Cloud Tasks Configuration
GCP_PROJECT_ID=
GCP_LOCATION=us-central1
GCP_TASKS_TIMEOUT=1800

# =============================================================================
# Worker System Configuration
# =============================================================================

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0
CELERY_WORKER_CONCURRENCY=4
CELERY_TASK_SERIALIZER=json

# RQ Configuration
RQ_CONNECTION_URL=redis://localhost:6379/0
RQ_DEFAULT_TIMEOUT=180
RQ_WORKER_TTL=420

# Dramatiq Configuration
DRAMATIQ_BROKER_URL=redis://localhost:6379/0
DRAMATIQ_MAX_RETRIES=20

# =============================================================================
# Use Case Defaults
# =============================================================================

# API Processing
API_PROCESSING_TIMEOUT=30
API_PROCESSING_CONCURRENCY=8
API_PROCESSING_RATE_LIMIT=100

# Data Pipeline
DATA_PIPELINE_TIMEOUT=1800
DATA_PIPELINE_CONCURRENCY=4
DATA_PIPELINE_BATCH_SIZE=100

# Email Sending
EMAIL_SENDING_TIMEOUT=60
EMAIL_SENDING_CONCURRENCY=16
EMAIL_SENDING_RATE_LIMIT=1000

# File Processing
FILE_PROCESSING_TIMEOUT=600
FILE_PROCESSING_CONCURRENCY=2

# Image Processing
IMAGE_PROCESSING_TIMEOUT=300
IMAGE_PROCESSING_CONCURRENCY=2

# Video Processing
VIDEO_PROCESSING_TIMEOUT=3600
VIDEO_PROCESSING_CONCURRENCY=1

# ML Inference
ML_INFERENCE_TIMEOUT=120
ML_INFERENCE_CONCURRENCY=4
ML_INFERENCE_BATCH_SIZE=32

# =============================================================================
# Deployment Configuration
# =============================================================================

# Standalone
STANDALONE_MAX_WORKERS=10
STANDALONE_MONITORING=true

# Docker
DOCKER_BASE_IMAGE=python:3.9-slim
DOCKER_HEALTH_CHECK=true

# Kubernetes
K8S_NAMESPACE=default
K8S_RESOURCE_REQUESTS_CPU=100m
K8S_RESOURCE_REQUESTS_MEMORY=128Mi
K8S_RESOURCE_LIMITS_CPU=500m
K8S_RESOURCE_LIMITS_MEMORY=512Mi

# Serverless
SERVERLESS_TIMEOUT=900
SERVERLESS_MEMORY=512

# =============================================================================
# Monitoring Configuration
# =============================================================================

# Prometheus
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=8000

# Health Checks
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=5

# Alerting
ALERT_WEBHOOK_URL=
ALERT_ON_QUEUE_SIZE=1000
ALERT_ON_FAILURE_RATE=0.1

# =============================================================================
# Testing Configuration
# =============================================================================

# Testing Features
ENABLE_TEST_MODE=true
TEST_QUEUE_PREFIX=test_
TEST_WORKER_COUNT=2

# Testing timeouts
ENABLE_QUEUE_TESTING=true
TEST_TASK_TIMEOUT=10

# =============================================================================
# Advanced Features
# =============================================================================

# Experimental Features
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_AUTO_SCALING=false
ENABLE_COST_OPTIMIZATION=false

# Performance Optimization
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_QUEUE_OPTIMIZATION=true
ENABLE_WORKER_OPTIMIZATION=true

# Integration Settings
ENABLE_WEBHOOK_INTEGRATION=true
WEBHOOK_RETRY_ATTEMPTS=3
WEBHOOK_TIMEOUT=30

# =============================================================================
# File Paths
# =============================================================================

# Project Paths
PROJECT_ROOT=.
OUTPUT_DIRECTORY=./generated
BACKUP_DIRECTORY=./backups
LOGS_DIRECTORY=./logs
CONFIG_DIRECTORY=./config

# =============================================================================
# Cloud Provider Credentials (Optional)
# =============================================================================

# AWS
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

# Azure
AZURE_SUBSCRIPTION_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_TENANT_ID=

# Google Cloud
GOOGLE_APPLICATION_CREDENTIALS=
GOOGLE_CLOUD_PROJECT=

# =============================================================================
# Queue-Specific Connection Strings
# =============================================================================

# Redis Connection Examples
REDIS_URL=redis://localhost:6379/0
REDIS_CLUSTER_URL=redis://localhost:7000,localhost:7001,localhost:7002

# RabbitMQ Connection Examples
RABBITMQ_URL=amqp://guest:guest@localhost:5672//
RABBITMQ_CLUSTER_URL=amqp://user:pass@node1:5672//,amqp://user:pass@node2:5672//

# Database Connections (for result backends)
DATABASE_URL=postgresql://user:password@localhost:5432/queue_db
MONGODB_URL=mongodb://localhost:27017/queue_db

# =============================================================================
# Worker Configuration Examples
# =============================================================================

# Celery Worker Settings
CELERY_WORKER_LOG_LEVEL=INFO
CELERY_WORKER_POOL=prefork
CELERY_WORKER_AUTOSCALE=10,2
CELERY_WORKER_MAX_TASKS_PER_CHILD=1000

# RQ Worker Settings
RQ_WORKER_NAME=default
RQ_WORKER_EXCEPTION_HANDLERS=
RQ_WORKER_CONNECTION_CLASS=

# Dramatiq Worker Settings
DRAMATIQ_WORKER_PROCESSES=1
DRAMATIQ_WORKER_THREADS=8

# =============================================================================
# Framework Integration
# =============================================================================

# FastAPI Integration
FASTAPI_TITLE="Queue Worker API"
FASTAPI_VERSION="1.0.0"
FASTAPI_DESCRIPTION="Queue and Worker Management API"

# Django Integration
DJANGO_SETTINGS_MODULE=myproject.settings
DJANGO_CELERY_APP=myproject

# Flask Integration
FLASK_APP=app.py
FLASK_ENV=development

# =============================================================================
# Monitoring and Alerting Integration
# =============================================================================

# Prometheus Configuration
PROMETHEUS_METRICS_PATH=/metrics
PROMETHEUS_REGISTRY=default

# Grafana Configuration
GRAFANA_URL=http://localhost:3000
GRAFANA_API_KEY=

# ElasticSearch/Kibana
ELASTICSEARCH_URL=http://localhost:9200
KIBANA_URL=http://localhost:5601

# Slack Integration
SLACK_WEBHOOK_URL=
SLACK_CHANNEL=#alerts

# Email Alerts
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
ALERT_EMAIL_FROM=alerts@yourcompany.com
ALERT_EMAIL_TO=devops@yourcompany.com

# =============================================================================
# Load Testing Configuration
# =============================================================================

# Load Testing Settings
LOAD_TEST_ENABLED=false
LOAD_TEST_WORKERS=5
LOAD_TEST_TASKS_PER_SECOND=10
LOAD_TEST_DURATION=60

# Performance Benchmarks
BENCHMARK_ENABLED=false
BENCHMARK_ITERATIONS=1000
BENCHMARK_CONCURRENCY=10

# =============================================================================
# Backup and Recovery
# =============================================================================

# Backup Configuration
BACKUP_ENABLED=false
BACKUP_INTERVAL_HOURS=24
BACKUP_RETENTION_DAYS=7
BACKUP_STORAGE_PATH=./backups

# Recovery Settings
RECOVERY_ENABLED=false
RECOVERY_CHECK_INTERVAL=300

# =============================================================================
# Cost Optimization
# =============================================================================

# Cost Monitoring
COST_MONITORING_ENABLED=false
COST_ALERT_THRESHOLD=100
COST_CURRENCY=USD

# Auto-shutdown
AUTO_SHUTDOWN_ENABLED=false
IDLE_SHUTDOWN_MINUTES=30
WEEKEND_SHUTDOWN=false

# =============================================================================
# Development and Testing Overrides
# =============================================================================

# Development Mode
DEVELOPMENT_MODE=false
DEV_SKIP_AUTH=false
DEV_MOCK_EXTERNAL_SERVICES=false

# Testing Overrides
TEST_SKIP_CLEANUP=false
TEST_USE_MEMORY_QUEUE=false
TEST_PARALLEL_WORKERS=2